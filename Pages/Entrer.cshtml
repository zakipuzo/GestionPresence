@page
@model GestionPresence.Pages.EntrerModel
@{
}


<p class="text-success">@Model.statusmsg</p>

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Input.coderfid" class="control-label"></label>
                <input asp-for="Input.coderfid" class="form-control" />
                <span asp-validation-for="Input.coderfid" class="text-danger"></span>
            </div>
       <div class="form-group">
                <label asp-for="Input.codepointeur" class="control-label"></label>
                <input asp-for="Input.codepointeur" class="form-control" />
                <span asp-validation-for="Input.codepointeur" class="text-danger"></span>
            </div>
          
            <div class="form-group">
                <input type="submit" value="Entrer" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
