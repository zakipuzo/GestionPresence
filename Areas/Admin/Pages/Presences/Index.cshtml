@page
@model gestionpresence.Areas.Admin.Pages.Presences.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create" asp-route-id="@Model.seanceid">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Presence[0].Inscription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Presence[0].Seance)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Presence) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Inscription.EtudiantId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seance.durree)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
