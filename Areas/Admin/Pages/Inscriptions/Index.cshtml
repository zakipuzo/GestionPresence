@page
@model gestionpresence.Areas.Admin.Pages.Inscriptions.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Les inscriptions de @Model.nomgroupe</h1>

<p>
    <a asp-page="Create" asp-route-id="@Model.groupeid">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
          
            <th>
               Etudiant
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Inscription) {
        <tr>
            
            <td>
                @item.Etudiant.Nom @item.Etudiant.Prenom
            </td>
          
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
